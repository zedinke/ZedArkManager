<Application x:Class="ZedASAManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:utilities="clr-namespace:ZedASAManager.Utilities"
             Startup="App_Startup">
    <Application.Resources>
        <utilities:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <utilities:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter"/>
        <utilities:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
        <utilities:BooleanToTextConverter x:Key="BooleanToTextConverter"/>
        <utilities:BooleanToColorConverter x:Key="BooleanToColorConverter"/>
        <utilities:StatusToColorConverter x:Key="StatusToColorConverter"/>
        <utilities:TimeRangeConverter x:Key="TimeRangeConverter"/>
        <utilities:StringToVisibilityConverter x:Key="StringToVisibilityConverter"/>
        <utilities:StringToBooleanConverter x:Key="StringToBooleanConverter"/>
        <utilities:BooleanLineToVisibilityConverter x:Key="BooleanLineToVisibilityConverter"/>
        <utilities:ValueLineToVisibilityConverter x:Key="ValueLineToVisibilityConverter"/>
        <utilities:CommentLineToVisibilityConverter x:Key="CommentLineToVisibilityConverter"/>
    </Application.Resources>
</Application>
